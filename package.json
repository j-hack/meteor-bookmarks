{
  "name": "meteor-rest-example",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "dependencies": {
    "meteor-node-stubs": "~0.2.0"
  },
  "description": "",
  "version": "0.1.0",
  "devDependencies": {
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.1.1"
  },
  "keywords": [],
  "author": "Nobutaka OSHIRO",
  "license": "MIT",
  "eslintConfig": {
     "plugins": [
       "meteor"
     ],
     "extends": [
       "airbnb",
       "plugin:meteor/recommended"
     ],
     "rules": {
       "meteor/eventmap-params": [
         2, { "templateInstanceParamName": "instance" }
       ],
       "import/no-unresolved": [
         2, { "ignore": ["^meteor/"] }
       ]
     }
   }
}
